<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="org.webkit.dumprendertree">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_SDCARD" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.webkit.permission.PLUGIN"/>

    <application android:hardwareAccelerated="true">
        <uses-library android:name="android.test.runner" />
        <activity android:name=".DumpRenderTreeActivity"
                  android:label="Dumps the render tree"
                  android:launchMode="singleTask"
                  android:theme="@style/WhiteBackground"
                  android:process=":rendertreedumper"
                  android:screenOrientation="landscape">
        </activity>
        <activity android:name=".DumpRenderTreeSWActivity"
                  android:label="Dumps the render tree with software"
                  android:launchMode="singleTask"
                  android:theme="@style/WhiteBackground"
                  android:process=":rendertreedumpersw"
                  android:hardwareAccelerated="false"
                  android:screenOrientation="landscape">
        </activity>
        <service android:name=".TestPlugin">
          <meta-data android:name="type" android:value="native" />
          <intent-filter>
            <action android:name="android.webkit.PLUGIN" />
          </intent-filter>
        </service>
    </application>
</manifest>
